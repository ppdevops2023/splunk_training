	"dataSources": {
		"ds_JeNUKcx2": {
			"type": "ds.search",
			"options": {
				"query": "index=\"covid19\" source=\"worldometer_coronavirus_summary_data.csv\" earliest=0 latest=now\n| table country ,continent, total_confirmed total_deaths, total_recovered active_cases, serious_or_critical, total_cases_per_1m_population, total_deaths_per_1m_population, total_tests total_tests_per_1m_population, population "
			},
			"name": "base_search_summary"
		},
		"ds_Folohgq9": {
			"type": "ds.chain",
			"options": {
				"extend": "ds_JeNUKcx2",
				"query": "| stats sum(active_cases) as total_active_cases \n| eval total_active_cases = round(total_active_cases / 1000000,2)"
			},
			"name": "current_active_cases"
		},
		"ds_2YVlhUM3": {
			"type": "ds.search",
			"options": {
				"query": "index=\"covid19\" source=\"worldometer_coronavirus_daily_data.csv\"\n| table _time,date, country ,cumulative_total_cases, daily_new_cases ,active_cases, cumulative_total_deaths ,daily_new_deaths"
			},
			"name": "base_search_daily"
		},
		"ds_UpAG8q9Z": {
			"type": "ds.chain",
			"options": {
				"extend": "ds_2YVlhUM3",
				"query": "| where country = \"India\"\n| timechart max(active_cases) as active_cases\n| eval active_cases = round(active_cases / 1000000,2)\n| fillnull"
			},
			"name": "active_cases_trend_india"
		},
		"ds_Mwomf7F6": {
			"type": "ds.chain",
			"options": {
				"extend": "ds_2YVlhUM3",
				"query": "| where country = \"USA\"\n| timechart max(active_cases) as active_cases\n| eval active_cases = round(active_cases / 1000000,2)\n| fillnull"
			},
			"name": "active_cases_tren_usa"
		},
		"ds_AQlFKzd1": {
			"type": "ds.chain",
			"options": {
				"extend": "ds_2YVlhUM3",
				"query": "| where country = \"Brazil\"\n| timechart max(active_cases) as active_cases\n| eval active_cases = round(active_cases / 1000000,2)\n| fillnull"
			},
			"name": "active_cases_trend_brazil"
		}
	},
